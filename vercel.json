{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ],
  "env": {
    "DATABASE_URL": "$DATABASE_URL",
    "SECRET_KEY": "$SECRET_KEY",
    "JWT_SECRET_KEY": "$JWT_SECRET_KEY"
  }
}
```